{
  "name": "datadao-frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "PORT=3000 NODE_ENV=development REACT_APP_BACKEND_URL=http://localhost:8080 react-scripts -r @cypress/instrument-cra start && open http://localhost:3000",
    "build": "react-scripts build",
    "docker-create": "doctl k8s cluster create --region fra1 datadao-frontend --wait && doctl kubernetes cluster kubeconfig save datadao-frontend",
    "docker-build": "yarn build && docker build -t neutrino/datadao-frontend . --no-cache",
    "docker-run": "docker run --rm -it -p 80:80 neutrino/datadao-frontend",
    "docker-push": "doctl registry login && docker tag neutrino/datadao-frontend registry.digitalocean.com/neutrino/datadao-frontend && docker push registry.digitalocean.com/neutrino/datadao-frontend",
    "docker-patch": "doctl registry kubernetes-manifest | kubectl --context do-fra1-datadao-frontend apply -f - && kubectl --context do-fra1-datadao-frontend patch serviceaccount default -p '{\"imagePullSecrets\": [{\"name\": \"registry-neutrino\"}]}'",
    "docker-secrets": "kubectl --context do-fra1-datadao-frontend create secret generic person --from-env-file=.env",
    "docker-deploy": "kubectl --context do-fra1-datadao-frontend apply -f kubernetesDeployment.json",
    "docker-serve": "kubectl --context do-fra1-datadao-frontend apply -f kubernetesService.json",
    "docker-restart": "kubectl --context do-fra1-datadao-frontend delete pods -l app=datadao-frontend",
    "docker-logs": "kubectl --context do-fra1-datadao-frontend logs --timestamps -l app=datadao-frontend",
    "docker-update": "yarn run docker-build && yarn run docker-push && yarn run docker-restart",
    "test": "react-scripts test",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:test": "cypress run -s 'cypress/integration/**/*-spec.js'",
    "import-shared": "rm -Rf src/shared && cp -R ../api/src/shared src/shared"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.0.10",
    "@daostack/arc.js": "2.0.0-experimental.58",
    "@dorgtech/daocreator-lib-experimental": "^1.1.12",
    "@dorgtech/daocreator-ui-experimental": "1.1.14",
    "@drizzle/react-components": "^1.5.3",
    "@drizzle/react-plugin": "^1.5.3",
    "@drizzle/store": "^1.5.3",
    "@oceanprotocol/react": "^0.4.0",
    "@openzeppelin/contracts": "^3.2.0",
    "@redux-offline/redux-offline": "^2.6.0",
    "@sentry/browser": "^5.18.0",
    "@textile/hub": "^5.2.0",
    "@truffle/hdwallet-provider": "^1.0.44",
    "antd": "^4.10.0",
    "axios": "^0.19.2",
    "bnc-notify": "^1.5.1",
    "chainlink": "^0.8.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "connected-react-router": "^6.8.0",
    "dayjs": "^1.8.28",
    "history": "4.10.1",
    "ipfs-http-client": "^45.0.0",
    "jwt-decode": "^2.2.0",
    "mobx": "^6.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.4",
    "react-google-recaptcha-v3": "^1.5.2",
    "react-helmet": "^6.1.0",
    "react-image-crop": "^8.6.4",
    "react-motion": "^0.5.2",
